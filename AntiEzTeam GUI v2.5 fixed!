-- ANTI EZ TEAM GUI V 2.8 - MOBILE FIXED
local function createGUI()
    local Players = game:GetService("Players")
    local player = Players.LocalPlayer
    local playerGui = player:WaitForChild("PlayerGui")
    local Lighting = game:GetService("Lighting")
    local TweenService = game:GetService("TweenService")
    local UserInputService = game:GetService("UserInputService")
    local RunService = game:GetService("RunService")

    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏—è –ø–ª–∞—Ç—Ñ–æ—Ä–º—ã
    local function isMobile()
        return UserInputService.TouchEnabled
    end

    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "AntiEzTeamGUI"
    ScreenGui.Parent = playerGui

    -- –û–°–ù–û–í–ù–û–ô –§–†–ï–ô–ú –î–õ–Ø –¢–ï–õ–ï–§–û–ù–ê
    local MainFrame = Instance.new("Frame")
    MainFrame.Name = "MainFrame"
    
    if isMobile() then
        -- –î–õ–Ø –¢–ï–õ–ï–§–û–ù–ê: –±–æ–ª—å—à–æ–π —Ñ—Ä–µ–π–º —Å –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–º –ø–æ–∫–∞–∑–æ–º –∫–Ω–æ–ø–æ–∫
        MainFrame.Size = UDim2.new(0.95, 0, 0.9, 0)
        MainFrame.Position = UDim2.new(0.025, 0, 0.05, 0)
        MainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
        MainFrame.BackgroundTransparency = 0.1
        MainFrame.BorderSizePixel = 0
    else
        -- –î–õ–Ø –ü–ö: –æ–±—ã—á–Ω—ã–π —Ä–∞–∑–º–µ—Ä
        MainFrame.Size = UDim2.new(0, 800, 0, 550)
        MainFrame.Position = UDim2.new(0.5, -400, 0.5, -275)
        MainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
        MainFrame.BackgroundTransparency = 0.1
        MainFrame.BorderSizePixel = 0
    end
    
    MainFrame.Visible = true
    MainFrame.Parent = ScreenGui

    -- –£–≥–ª—ã –¥–ª—è –∫—Ä–∞—Å–æ—Ç—ã
    local mainCorner = Instance.new("UICorner")
    mainCorner.CornerRadius = UDim.new(0, 12)
    mainCorner.Parent = MainFrame

    local mainStroke = Instance.new("UIStroke")
    mainStroke.Color = Color3.fromRGB(100, 100, 200)
    mainStroke.Thickness = 2
    mainStroke.Parent = MainFrame

    -- –ó–ê–ì–û–õ–û–í–û–ö
    local TitleLabel = Instance.new("TextLabel")
    TitleLabel.Name = "TitleLabel"
    TitleLabel.Size = UDim2.new(1, 0, 0, isMobile() and 60 or 50)
    TitleLabel.Position = UDim2.new(0, 0, 0, 0)
    TitleLabel.BackgroundColor3 = Color3.fromRGB(40, 40, 80)
    TitleLabel.Text = isMobile() and "üì± ANTIEZTEAM MOBILE" or "‚ú® ANTIEZTEAM GUI V 2.8 ‚ú®"
    TitleLabel.TextColor3 = Color3.new(1, 1, 1)
    TitleLabel.TextScaled = true
    TitleLabel.Font = Enum.Font.GothamBold
    TitleLabel.Parent = MainFrame

    local titleCorner = Instance.new("UICorner")
    titleCorner.CornerRadius = UDim.new(0, 12)
    titleCorner.Parent = TitleLabel

    -- –ö–ù–û–ü–ö–ê –°–ö–†–´–¢–ò–Ø –î–õ–Ø –¢–ï–õ–ï–§–û–ù–ê (–≤ –∑–∞–≥–æ–ª–æ–≤–∫–µ)
    local CloseButton = Instance.new("TextButton")
    CloseButton.Name = "CloseButton"
    CloseButton.Size = UDim2.new(0, 40, 0, 40)
    CloseButton.Position = UDim2.new(0.9, 0, 0.1, 0)
    CloseButton.BackgroundColor3 = Color3.fromRGB(200, 60, 60)
    CloseButton.Text = "‚úï"
    CloseButton.TextColor3 = Color3.new(1, 1, 1)
    CloseLabel.TextScaled = true
    CloseButton.Visible = isMobile()
    CloseButton.Parent = TitleLabel

    local closeCorner = Instance.new("UICorner")
    closeCorner.CornerRadius = UDim.new(1, 0)
    closeCorner.Parent = CloseButton

    -- –û–°–ù–û–í–ù–û–ô –ö–û–ù–¢–ï–ô–ù–ï–† –î–õ–Ø –ö–ù–û–ü–û–ö –ò –°–ö–†–ò–ü–¢–û–í
    local ContentFrame = Instance.new("Frame")
    ContentFrame.Name = "ContentFrame"
    ContentFrame.Size = UDim2.new(1, 0, 1, isMobile() and -60 or -50)
    ContentFrame.Position = UDim2.new(0, 0, 0, isMobile() and 60 or 50)
    ContentFrame.BackgroundTransparency = 1
    ContentFrame.Parent = MainFrame

    if isMobile() then
        -- –î–õ–Ø –¢–ï–õ–ï–§–û–ù–ê: –∫–Ω–æ–ø–∫–∏ –≤—Å–µ–≥–¥–∞ –≤–∏–¥–Ω—ã —Å–ª–µ–≤–∞, —Å–∫—Ä–∏–ø—Ç —Å–ø—Ä–∞–≤–∞
        -- –§–†–ï–ô–ú –ö–ù–û–ü–û–ö (–≤—Å–µ–≥–¥–∞ –≤–∏–¥–µ–Ω –Ω–∞ —Ç–µ–ª–µ—Ñ–æ–Ω–µ)
        local ButtonsFrame = Instance.new("Frame")
        ButtonsFrame.Name = "ButtonsFrame"
        ButtonsFrame.Size = UDim2.new(0.4, 0, 1, 0)
        ButtonsFrame.Position = UDim2.new(0, 0, 0, 0)
        ButtonsFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
        ButtonsFrame.BackgroundTransparency = 0.1
        ButtonsFrame.BorderSizePixel = 0
        ButtonsFrame.Parent = ContentFrame

        local buttonsCorner = Instance.new("UICorner")
        buttonsCorner.CornerRadius = UDim.new(0, 10)
        buttonsCorner.Parent = ButtonsFrame

        -- SCROLLING FRAME –î–õ–Ø –ö–ù–û–ü–û–ö
        local ButtonsScrollingFrame = Instance.new("ScrollingFrame")
        ButtonsScrollingFrame.Name = "ButtonsScrollingFrame"
        ButtonsScrollingFrame.Size = UDim2.new(1, 0, 1, -50)
        ButtonsScrollingFrame.Position = UDim2.new(0, 0, 0, 0)
        ButtonsScrollingFrame.BackgroundTransparency = 1
        ButtonsScrollingFrame.ScrollBarThickness = 4
        ButtonsScrollingFrame.ScrollBarImageColor3 = Color3.fromRGB(100, 100, 200)
        ButtonsScrollingFrame.Parent = ButtonsFrame

        local UIListLayout = Instance.new("UIListLayout")
        UIListLayout.Padding = UDim.new(0, 5)
        UIListLayout.Parent = ButtonsScrollingFrame

        -- TEXTBOX –î–õ–Ø –°–ö–†–ò–ü–¢–û–í (—Å–ø—Ä–∞–≤–∞)
        local MainScriptBox = Instance.new("TextBox")
        MainScriptBox.Name = "MainScriptBox"
        MainScriptBox.Size = UDim2.new(0.55, 0, 0.7, 0)
        MainScriptBox.Position = UDim2.new(0.42, 0, 0, 0)
        MainScriptBox.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
        MainScriptBox.TextColor3 = Color3.new(1, 1, 1)
        MainScriptBox.PlaceholderText = "–í–≤–µ–¥–∏—Ç–µ —Å–∫—Ä–∏–ø—Ç..."
        MainScriptBox.Text = ""
        MainScriptBox.TextXAlignment = Enum.TextXAlignment.Left
        MainScriptBox.TextYAlignment = Enum.TextYAlignment.Top
        MainScriptBox.MultiLine = true
        MainScriptBox.TextSize = 14
        MainScriptBox.Font = Enum.Font.Code
        MainScriptBox.ScrollBarThickness = 6
        MainScriptBox.Parent = ContentFrame

        local scriptBoxCorner = Instance.new("UICorner")
        scriptBoxCorner.CornerRadius = UDim.new(0, 8)
        scriptBoxCorner.Parent = MainScriptBox

        -- –ö–ù–û–ü–ö–ê EXECUTE
        local MainExecuteButton = Instance.new("TextButton")
        MainExecuteButton.Name = "MainExecuteButton"
        MainExecuteButton.Size = UDim2.new(0.55, 0, 0, 40)
        MainExecuteButton.Position = UDim2.new(0.42, 0, 0.75, 0)
        MainExecuteButton.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
        MainExecuteButton.Text = "‚ö° –í–´–ü–û–õ–ù–ò–¢–¨"
        MainExecuteButton.TextColor3 = Color3.new(1, 1, 1)
        MainExecuteButton.TextScaled = true
        MainExecuteButton.Font = Enum.Font.GothamBold
        MainExecuteButton.Parent = ContentFrame

        local executeCorner = Instance.new("UICorner")
        executeCorner.CornerRadius = UDim.new(0, 10)
        executeCorner.Parent = MainExecuteButton

        -- –ö–ù–û–ü–ö–ê –í–´–ë–û–†–ê –ò–ì–†
        local SelectGameButton = Instance.new("TextButton")
        SelectGameButton.Name = "SelectGameButton"
        SelectGameButton.Size = UDim2.new(0.55, 0, 0, 40)
        SelectGameButton.Position = UDim2.new(0.42, 0, 0.85, 0)
        SelectGameButton.BackgroundColor3 = Color3.fromRGB(255, 150, 50)
        SelectGameButton.Text = "üéÆ –ò–ì–†–´"
        SelectGameButton.TextColor3 = Color3.new(1, 1, 1)
        SelectGameButton.TextScaled = true
        SelectGameButton.Font = Enum.Font.GothamBold
        SelectGameButton.Parent = ContentFrame

        local gameCorner = Instance.new("UICorner")
        gameCorner.CornerRadius = UDim.new(0, 10)
        gameCorner.Parent = SelectGameButton

    else
        -- –î–õ–Ø –ü–ö: —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω–∞—è –≤–µ—Ä—Å–∏—è
        -- ... (–≤–∞—à –æ–±—ã—á–Ω—ã–π –∫–æ–¥ –¥–ª—è –ü–ö)
    end

    -- –°–ü–ò–°–û–ö –ö–ù–û–ü–û–ö (—Ç–∞–∫–æ–π –∂–µ –∫–∞–∫ —Ä–∞–Ω—å—à–µ)
    local buttonList = {
        {"üîß FE CADUCUS", Color3.fromRGB(200, 50, 50), function()
            loadstring(game:HttpGet("https://pastebin.com/raw/XJeBNe0s"))()
        end},
        {"üå™Ô∏è NATURAL DISASTER", Color3.fromRGB(50, 150, 50), function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/antiezteam/-/refs/heads/main/fwwafwfaf"))()
        end},
        {"‚ö° INFINITY YIELD", Color3.fromRGB(50, 50, 200), function()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
        end},
        {"üëÅÔ∏è REMOTE SPY", Color3.fromRGB(180, 80, 220), function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/78n/SimpleSpy/main/SimpleSpySource.lua"))()
        end},
        {"üõ°Ô∏è FE BYPASS", Color3.fromRGB(30, 180, 200), function()
            loadstring(game:HttpGet("https://pastebin.com/raw/1234567890"))()
        end},
        {"üåé SERVERSIDE", Color3.fromRGB(230, 150, 30), function()
            -- –§—É–Ω–∫—Ü–∏—è –∏–∑–º–µ–Ω–µ–Ω–∏—è –Ω–µ–±–∞
            local originalAmbient = Lighting.Ambient
            local originalOutdoor = Lighting.OutdoorAmbient
            Lighting.Ambient = Color3.new(1, 0, 0)
            Lighting.OutdoorAmbient = Color3.new(1, 0, 0)
            wait(10)
            Lighting.Ambient = originalAmbient
            Lighting.OutdoorAmbient = originalOutdoor
        end},
        {"üîç SCAN REMOTES", Color3.fromRGB(50, 50, 180), function()
            for i,v in pairs(game:GetDescendants()) do
                if v:IsA("RemoteEvent") or v:IsA("RemoteFunction") then
                    print("–ù–∞–π–¥–µ–Ω Remote: " .. v:GetFullName())
                end
            end
        end},
        {"üé® DECAL", Color3.fromRGB(200, 100, 30), function()
            local decalId = "rbxassetid://1234567890"
            for i,v in pairs(workspace:GetDescendants()) do
                if v:IsA("Part") and v.Name == "Baseplate" then
                    local decal = Instance.new("Decal")
                    decal.Texture = decalId
                    decal.Face = Enum.NormalId.Top
                    decal.Parent = v
                end
            end
        end},
        {"üõ†Ô∏è BTOOLS", Color3.fromRGB(150, 80, 200), function()
            local tool1 = Instance.new("HopperBin")
            tool1.BinType = Enum.BinType.Hammer
            tool1.Parent = player.Backpack
            local tool2 = Instance.new("HopperBin")
            tool2.BinType = Enum.BinType.Clone
            tool2.Parent = player.Backpack
            local tool3 = Instance.new("HopperBin")
            tool3.BinType = Enum.BinType.Grab
            tool3.Parent = player.Backpack
        end},
        {"üíç SUPER RING V4", Color3.fromRGB(230, 80, 130), function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/antiezteam/-/refs/heads/main/super%20ring%20part%20v4"))()
        end}
    }

    -- –°–û–ó–î–ê–ï–ú –ö–ù–û–ü–ö–ò –î–õ–Ø –¢–ï–õ–ï–§–û–ù–ê
    if isMobile() and ButtonsScrollingFrame then
        for i, buttonData in ipairs(buttonList) do
            local buttonName, buttonColor, buttonFunction = buttonData[1], buttonData[2], buttonData[3]
            
            local button = Instance.new("TextButton")
            button.Name = buttonName .. "Button"
            button.Size = UDim2.new(0.95, 0, 0, 35)
            button.Position = UDim2.new(0.025, 0, 0, (i-1)*40)
            button.BackgroundColor3 = buttonColor
            button.Text = buttonName
            button.TextColor3 = Color3.new(1, 1, 1)
            button.TextScaled = true
            button.Font = Enum.Font.Gotham
            button.Parent = ButtonsScrollingFrame

            local btnCorner = Instance.new("UICorner")
            btnCorner.CornerRadius = UDim.new(0, 6)
            btnCorner.Parent = button

            button.MouseButton1Click:Connect(function()
                print(buttonName .. " –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω!")
                buttonFunction()
            end)
        end

        -- –û–ë–ù–û–í–õ–Ø–ï–ú –†–ê–ó–ú–ï–† SCROLLING FRAME
        ButtonsScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, UIListLayout.AbsoluteContentSize.Y)
    end

    -- –§–£–ù–ö–¶–ò–ò –î–õ–Ø –ö–ù–û–ü–û–ö
    if MainExecuteButton then
        MainExecuteButton.MouseButton1Click:Connect(function()
            local scriptText = MainScriptBox.Text
            if scriptText and scriptText ~= "" then
                local success, errorMessage = pcall(function()
                    loadstring(scriptText)()
                end)
                if not success then
                    warn("–û—à–∏–±–∫–∞: " .. errorMessage)
                end
            end
        end)
    end

    if SelectGameButton then
        SelectGameButton.MouseButton1Click:Connect(function()
            -- –§—É–Ω–∫—Ü–∏—è –≤—ã–±–æ—Ä–∞ –∏–≥—Ä
            print("–ú–µ–Ω—é –∏–≥—Ä...")
        end)
    end

    if CloseButton then
        CloseButton.MouseButton1Click:Connect(function()
            MainFrame.Visible = false
        end)
    end

    -- –ö–ù–û–ü–ö–ê –û–¢–ö–†–´–¢–ò–Ø GUI –î–õ–Ø –¢–ï–õ–ï–§–û–ù–ê
    if isMobile() then
        local OpenGUIButton = Instance.new("TextButton")
        OpenGUIButton.Name = "OpenGUIButton"
        OpenGUIButton.Size = UDim2.new(0, 80, 0, 80)
        OpenGUIButton.Position = UDim2.new(0, 10, 0.5, -40)
        OpenGUIButton.BackgroundColor3 = Color3.fromRGB(40, 40, 80)
        OpenGUIButton.Text = "üì±\nGUI"
        OpenGUIButton.TextColor3 = Color3.new(1, 1, 1)
        OpenGUIButton.TextScaled = true
        OpenGUIButton.Visible = not MainFrame.Visible
        OpenGUIButton.ZIndex = 10
        OpenGUIButton.Parent = ScreenGui

        local openCorner = Instance.new("UICorner")
        openCorner.CornerRadius = UDim.new(1, 0)
        openCorner.Parent = OpenGUIButton

        OpenGUIButton.MouseButton1Click:Connect(function()
            MainFrame.Visible = true
            OpenGUIButton.Visible = false
        end)

        CloseButton.MouseButton1Click:Connect(function()
            MainFrame.Visible = false
            OpenGUIButton.Visible = true
        end)
    end

    print("üì± ANTIEZTEAM MOBILE GUI –∑–∞–≥—Ä—É–∂–µ–Ω–æ!")
    if isMobile() then
        print("‚úÖ –ê–¥–∞–ø—Ç–∏—Ä–æ–≤–∞–Ω–æ –¥–ª—è —Ç–µ–ª–µ—Ñ–æ–Ω–∞")
        print("‚úÖ –ö–Ω–æ–ø–∫–∏ –≤—Å–µ–≥–¥–∞ –≤–∏–¥–Ω—ã")
        print("‚úÖ –û—Ç–¥–µ–ª—å–Ω–∞—è –∫–Ω–æ–ø–∫–∞ –æ—Ç–∫—Ä—ã—Ç–∏—è")
    end
end

createGUI()
