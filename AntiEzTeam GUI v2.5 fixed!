    ToggleButtonsButton.Name = "ToggleButtonsButton"
    ToggleButtonsButton.Size = isMobile() and UDim2.new(0.4, 0, 0, 45) or UDim2.new(0.12, 0, 0, 35)
    ToggleButtonsButton.Position = isMobile() and UDim2.new(0.05, 0, 0.65, 0) or UDim2.new(0.02, 0, 0.8, 0)
    ToggleButtonsButton.BackgroundColor3 = Color3.fromRGB(80, 80, 160)
    ToggleButtonsButton.Text = isMobile() and "üìã –ö–ù–û–ü–ö–ò" or "üìã –ü–û–ö–ê–ó–ê–¢–¨ –ö–ù–û–ü–ö–ò"
    ToggleButtonsButton.TextColor3 = Color3.new(1, 1, 1)
    ToggleButtonsButton.TextScaled = true
    ToggleButtonsButton.Font = Enum.Font.GothamBold
    ToggleButtonsButton.Parent = MainFrame

    local toggleCorner = Instance.new("UICorner")
    toggleCorner.CornerRadius = UDim.new(0, 10)
