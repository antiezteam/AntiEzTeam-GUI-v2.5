void setup() {
  // Инициализация пина LED_BUILTIN как выхода
  pinMode(LED_BUILTIN, OUTPUT);
}

void loop() {
  // Включаем светодиод (устанавливаем высокий уровень напряжения)
  digitalWrite(LED_BUILTIN, HIGH);
  // Ждем 1000 миллисекунд (1 секунда)
  delay(1000);
  // Выключаем светодиод (устанавливаем низкий уровень напряжения)
  digitalWrite(LED_BUILTIN, LOW);
  // Ждем еще 1000 миллисекунд
  delay(1000);
}
