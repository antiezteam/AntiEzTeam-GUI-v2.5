	-- ФУНКЦИЯ ПЕРЕМЕЩЕНИЯ ТОЛЬКО ДЛЯ ПК
	local function makeDraggable(gui)
		local dragging = false
		local dragInput, dragStart, startPos

		gui.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true
				dragStart = input.Position
				startPos = gui.Position

				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)

		gui.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement then
				dragInput = input
			end
		end)

		UserInputService.InputChanged:Connect(function(input)
			if input == dragInput and dragging then
				local delta = input.Position - dragStart
				gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
			end
		end)
	end

	-- ФУНКЦИЯ ДЛЯ СКРЫТИЯ/ПОКАЗА GUI
	local function toggleGUI()
		if MainFrame.Visible then
			-- Скрываем GUI с анимацией
			local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
			local tween = TweenService:Create(MainFrame, tweenInfo, {Position = UDim2.new(0.5, -425, -1, -250)})
			tween:Play()

			wait(0.3)
			MainFrame.Visible = false
			ToggleGUIButton.Text = "GUI +"
			print("GUI скрыто")
		else
			-- Показываем GUI с анимацией
			MainFrame.Visible = true
			local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
			local tween = TweenService:Create(MainFrame, tweenInfo, {Position = UDim2.new(0.5, -425, 0.5, -250)})
			tween:Play()

			ToggleGUIButton.Text = "GUI -"
			print("GUI показано")
		end
	end

	-- ГОРЯЧАЯ КЛАВИША ДЛЯ СКРЫТИЯ GUI (Right Control)
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end

		if input.KeyCode == Enum.KeyCode.RightControl then
			toggleGUI()
		end
	end)

